{
  "presets": [
    "@babel/react",
    "@babel/env"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties"
    ],
    [
      "@babel/plugin-transform-runtime"
    ],
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "extensions": [
          ".css",
          ".ts",
          ".tsx",
          ".jsx",
          ".js",
          ".json"
        ]
      }
    ]
  ],
  "ignore": [
    ".css",
    ".scss"
  ]
}